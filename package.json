{
  "name": "stellar-terminal",
  "version": "0.0.0",
  "author": "Iris Li",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "install": "npm install && ./node_modules/.bin/bower install",
    "start": "./node_modules/.bin/gulp watch",
    "production": "NODE_ENV=production; ./node_modules/.bin/gulp production"
  },
  "dependencies": {},
  "devDependencies": {
    "babelify": "^6.1.3",
    "browser-sync": "latest",
    "browserify": "latest",
    "del": "~0.1.3",
    "gulp": ">=3.8.8",
    "gulp-autoprefixer": "~1.0.1",
    "gulp-bower": "0.0.6",
    "gulp-cache": "~0.2.4",
    "gulp-exit": "0.0.2",
    "gulp-inline-source": "^3.0.0",
    "gulp-load-plugins": "~0.7.0",
    "gulp-strip-debug": "^1.0.2",
    "gulp-uglify": "^1.0.2",
    "gulp-useref": "~0.4.4",
    "gulp-util": "~3.0.1",
    "jest-cli": "^18.1.0",
    "main-bower-files": "~2.6.2",
    "react": "latest",
    "react-dom": "latest",
    "react-tools": "latest",
    "reactify": "latest",
    "run-sequence": "^1.2.2",
    "standard": "^8.6.0",
    "strip-debug": "^1.0.1",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "~2.1"
  },
  "engines": {
    "node": ">=7.3.0"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "ignore": [
            "bower_components"
          ]
        }
      ]
    ]
  },
  "browser": {
    "jquery": "./app/bower_components/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$"
  }
}
